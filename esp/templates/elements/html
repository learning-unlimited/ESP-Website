    djConfig = {
        modulePaths: {
            "dojo": "https://ajax.googleapis.com/ajax/libs/dojo/1.6.1/dojo",
            "dijit": "https://ajax.googleapis.com/ajax/libs/dojo/1.6.1/dijit",
            "dojox": "https://ajax.googleapis.com/ajax/libs/dojo/1.6.1/dojox"
        },
        parseOnLoad: true
    };
    </script>
